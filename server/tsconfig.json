{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": "..",
    "typeRoots": [
      "../node_modules/@types",
      "./node_modules/@types"
    ]
  },
  "include": [
    "src",
    "../shared/generated/prisma",
    "../shared/dist"
  ]
}